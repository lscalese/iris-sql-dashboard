          "hide": false,
          "query": "from(bucket: \"IRIS_SQL_STATS\")\r\n  |> range(start: $__from/1000, stop: $__to/1000)\r\n  |> filter(fn: (r) => r._measurement == \"sqlstats_daily\" \r\n    and r.instance_name == \"${iris_instance_name}\" \r\n    and r.server_name == \"${iris_server_name}\" \r\n    and r.namespace == \"${iris_namespace}\"\r\n  )\r\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\r\n  |> keep(columns: [\"_time\", \"count\", \"commands\", \"execution_time\", \"row_count\", \"statement\", \"hash\"])\r\n  |> filter(fn: (r) => r.hash == \"${iris_sql_hash}\")\r\n",
          "refId": "A"
        }
      ],
